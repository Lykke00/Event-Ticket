<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="mainWindow" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="easv.event.gui.MainController">
   <top>
      <HBox alignment="CENTER_LEFT" prefHeight="51.0" prefWidth="731.0" spacing="5.0" styleClass="header-tab-pane">
         <children>
            <HBox fx:id="hBoxLogo" alignment="CENTER_LEFT" minWidth="-Infinity" prefHeight="50.0" prefWidth="79.0">
               <children>
                  <ImageView fitHeight="32.0" fitWidth="78.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/logo.png" />
                     </image>
                  </ImageView>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </HBox>
            <HBox fx:id="hBoxTabs" alignment="CENTER_RIGHT" prefHeight="52.0" prefWidth="195.0" spacing="30.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="btnEventsPage" maxHeight="1.7976931348623157E308" mnemonicParsing="false" styleClass="tab" text="Events">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font></Button>
                  <Button fx:id="btnTicketPage" maxHeight="1.7976931348623157E308" mnemonicParsing="false" styleClass="tab" text="Billetter" HBox.hgrow="ALWAYS">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font></Button>
                  <Button fx:id="btnUsersPage" maxHeight="1.7976931348623157E308" mnemonicParsing="false" styleClass="tab" text="Brugere">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font></Button>
                  <Button fx:id="btnLogOut" maxHeight="1.7976931348623157E308" mnemonicParsing="false" styleClass="tab" text="Log ud">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font></Button>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets right="5.0" />
         </padding>
      </HBox>
   </top>
</BorderPane>